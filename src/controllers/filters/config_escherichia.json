{
  "name": "E. coli Inpatient Analysis",
  "description": "E. coli, first isolates, inpatient only, 2019-2023",
  "filters": [
    {
      "column": "PathogenGenus",
      "operator": "equals",
      "value": "Escherichia"
    },
  
    {
        "column": "ARS_WardType",
        "operator": "in",
        "values": ["Normal Ward", "Intensive Care Unit", "Outpatient"]
    },
    {
      "column": "CareType",
      "operator": "in",
      "values": ["In-Patient", "Out-Patient"]
    },
    {
      "column": "Year",
      "operator": "range",
      "min": 2019,
      "max": 2023
    }
  ],
  "verbose": true
}
